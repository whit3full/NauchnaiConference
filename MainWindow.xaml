<Window x:Class="NauchnaiConference.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NauchnaiConference"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen" MinHeight="450" MinWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="4*"/>
        </Grid.RowDefinitions>
        <Grid Background="LightBlue" Grid.Row="0"/>
        <Button x:Name="btnBack" Content="Назад" HorizontalAlignment="Right" Margin="0 0 10 0" Click="btnBack_Click"/>
        <Label Content="Научная конференция"  FontSize="28"
               HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Frame x:Name="frameMain" Grid.Row="1" ContentRendered="frameMain_ContentRendered" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
